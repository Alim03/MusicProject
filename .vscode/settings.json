    "editor.fontSize": 14,
    "python.languageServer": "Pylance",
        "version": "0.2.0",
        "configurations": [
          {
            "name": "Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
              "--extensionDevelopmentPath=${workspaceFolder}"
            ]
          }
        ],
        "emmet.includeLanguages": {"django-html":"html"}
}